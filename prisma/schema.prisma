generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./migrations/test.db"
}

model records {
  id    String  @id @default(uuid())
  title String
  time  Int
  created_at DateTime @default(now())
}